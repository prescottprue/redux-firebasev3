{
  "name": "devshare-site",
  "version": "0.1.3",
  "main": "bin/server.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build-bundle && npm run build-server && npm run copy-assets",
    "build-bundle": "webpack -p --stats --config webpack-production.config",
    "build-server": "webpack -p --stats --config webpack-server-production.config",
    "copy-assets": "cp -a assets build",
    "test": "mocha -R spec --compilers js:babel-core/register ./test/setup.js ./test/**/*.js",
    "start": "node bin/dev-server",
    "production": "node bin/server"
  },
  "keywords": [
    "webpack",
    "redux",
    "react",
    "react-router",
    "firebase",
    "redux-firebasev3"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "firebase": "3.3.0",
    "history": "1.13.0",
    "json-loader": "0.5.4",
    "lodash": "^4.15.0",
    "material-ui": "0.15.4",
    "proxy-middleware": "0.15.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "2.1.2",
    "react-router": "2.0.0-rc6",
    "react-router-redux": "3.0.0",
    "react-tap-event-plugin": "0.2.1",
    "redux": "3.4.0",
    "redux-firebasev3": "0.1.0",
    "redux-router": "1.0.0-beta3",
    "redux-thunk": "2.0.1",
    "serve-static": "1.10.0",
    "source-map-support": "0.4.0",
    "webpack": "1.13.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.8",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.16.0",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5",
    "node-sass": "^3.3.2",
    "react-hot-loader": "^2.0.0-alpha-4",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "webpack-dev-server": "1.14.0"
  }
}
